<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="startColor" format="color" />
    <attr name="endColor" format="color" />
    <attr name="level" format="integer" />
    <declare-styleable name="SierpinskiCurveView">
        <attr name="level" />
        <attr name="distance" format="dimension" />
    </declare-styleable>
    <declare-styleable name="PascalTriangleView">
        <attr name="triangleHeight" format="integer" />
        <attr name="itemColor" format="color" />
        <attr name="itemTextColor" format="color" />
    </declare-styleable>
    <declare-styleable name="PythagorasTreeView">
        <attr name="startColor" />
        <attr name="endColor" />
        <attr name="maxDepth" format="integer" />
    </declare-styleable>
    <declare-styleable name="KochSnowView">
        <attr name="startColor" />
        <attr name="endColor" />
        <attr name="level" />
    </declare-styleable>
    <declare-styleable name="SierpinskiTriangleView">
        <attr name="color1" format="color" />
        <attr name="color2" format="color" />
        <attr name="depth" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomTableView">
        <attr name="selectColor" format="color" />
        <attr name="textColor" format="color"/>
        <attr name="android:textSize"/>
    </declare-styleable>
</resources>